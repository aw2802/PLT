public class Car{
  public String color;

  Car(){
    color = "green";
  }

  public String getColor(){
    return color;
  }
}

public class TestArrayObject{
  public void main(){
    Car myCar = new Car();
    Car[] carArray = new Car[5];
    carArray[0] = myCar;
    println(carArray[0].getColor());
  }
}

public class TestIfNested{
  public void main(){
    int i = 2;
    int j = 4;
    if (true){
      if (i == 2){
        if (i < j){
          print(j);
        }
        else{
          print("In else 1");
        }
      }
      else{
        print("In else 2");
      }
    }
    else{
      print("In else 3");
